{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
    "arrowParens": "always",
    "semi": false,
    "tabWidth": 2,
    "bracketSpacing": true,
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
